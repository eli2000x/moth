    <script type="text/javascript">
        google.charts.load("current", {packages: ["corechart, bar"]});

        google.charts.setOnLoadCallback(drawBasic);

        function drawBasic() {


            const arr = [
                ["Priority", "Number of tickets", {role: "style"}],
                ["New", 2, "#007bff"],
                ["Open", 1, "#28a745"],
                ["In Progress", 2, "#ffc107"],
                ["Resolved", 1, "#dc3545"],
                ["Need More Info", 1, "#123456"]
            ]
    
            var data = google.visualization.arrayToDataTable(arr);

            var options = {
                "title": ' Project Tickets by Status',
                "hAxis": {textStyle: {fontSize: 13}},
                "chartArea": {width: '80%'},
                "backgroundColor": "#eceff1",
                "legend": {position: "none"},
                "fontName": "Raleway",
                "fontSize": 22,
                "height": 400
                
                
            };

            var chart = new google.visualization.ColumnChart(document.getElementById('tickets_status'));

            chart.draw(data, options);
        }
    </script>

    <script type="text/javascript">
        google.charts.load("current", {packages: ["corechart"]});

        google.charts.setOnLoadCallback(drawBasic);

        function drawBasic() {


            const arr = [
                ["Priority", "Number of tickets", {role: "style"}],
                ["Bug", 3, "#007bff"],
                ["Task", 1, "#28a745"],
                ["Feature", 1, "#ffc107"],
                ["Improvement", 2, "#dc3545"]
            ]

            var data = google.visualization.arrayToDataTable(arr);

            var options = {
                "title": ' Project Tickets by Type',
                "chartArea": {width: '80%'},
                "backgroundColor": "#eceff1",
                "fontName": "Raleway",
                "legend": {position: "bottom", textStyle: {fontSize: 15}},
                "fontSize": 22,
                "height": 400,
                "pieSliceText": "value", 
                "pieHole": 0.4
                
                
            };

            var chart = new google.visualization.PieChart(document.getElementById('tickets_type'));

            chart.draw(data, options);
        }
    </script>

